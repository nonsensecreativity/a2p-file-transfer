<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.olayinka.file.transfer.model.AppInfo">
        <table name="APP_INFO"/>
        <attributes>
            <id name="id">
                <column name="id" nullable="false" precision="19" unique="true"/>
            </id>
            <basic name="name">
                <column name="name" nullable="false" length="256"/>
            </basic>
            <basic name="dbVersion">
                <column name="db_version" nullable="false" precision="19"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
